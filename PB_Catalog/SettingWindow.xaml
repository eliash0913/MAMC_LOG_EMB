<Window x:Class="PB_Catalog.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PB_Catalog"
        mc:Ignorable="d"
        Title="Setting" Height="78.689" Width="777.869">
    <Grid>
        <Label Content="Upload location" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="UploadPathBox" HorizontalAlignment="Left" Height="26" Margin="119,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="387"/>
        <Button x:Name="Setting_Browse_Button" Content="Browse" HorizontalAlignment="Left" Margin="521,10,0,0" VerticalAlignment="Top" Width="75" Height="26" Click="Browse_Button_Click"/>
        <Button x:Name="Setting_Save_Button" Content="Save" HorizontalAlignment="Left" Margin="601,10,0,0" Width="75" Click="Save_Button_Click" Height="26" VerticalAlignment="Top"/>
        <Button x:Name="Setting_Cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="681,10,0,0" VerticalAlignment="Top" Width="75" Height="26" Click="Cancel_Button_Click"/>

    </Grid>
</Window>
